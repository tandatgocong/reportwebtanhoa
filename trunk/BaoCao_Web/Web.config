      providerName="System.Data.SqlClient" />
    <add name="DocSo_PHTConnectionString" connectionString="Data Source=HP_G7;Initial Catalog=DocSo_PHT;Persist Security Info=True;User ID=sa;Password=123@tanhoa"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>